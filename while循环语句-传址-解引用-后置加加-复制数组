char* my_strcpy(const char *ar1, char *ar2)
{
	assert(ar1);
	assert(ar2);
	char* ret = ar1;//用指针可以保证地址不会被改变，虽然注释掉，不用也可以实现功能
	while (*ar2++ = *ar1++)
	{ }
	//return ret;
}
int main()
{
	char arr1[] = "abcdef";
	char arr2[20] = { 0 };
	my_strcpy(arr1, arr2);
	printf("%s\n", arr1);
	printf("%s\n", arr2);
	return 0;
}
