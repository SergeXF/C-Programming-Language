int main()
{
	char ch = 0;
	while (scanf(" %c", &ch) == 1)//前面的空格可以跳过空白字符，否则循环过后，会读取\n，导致错误
	{
		if ((ch >= 'A' && ch <= 'Z') || (ch >= 'a' && ch <= 'z'))
		{
			printf("%c is an alphabet.\n", ch);
		}
		else 
		{
			printf("%c is not an alphabet.\n", ch);
		}
		//getchar();//这个到底是啥作用啊啊啊啊啊啊？？？？？？？？？？？？？？？？？？？？？？？？？？？？？？？？？？？？？？？？？？？？？鹏哥76集23分
	}
	return 0;
}
