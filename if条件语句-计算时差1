int main()
{
	int hour1, minute1;
	int hour2, minute2;
	int ih, im;
	printf("起始时间");
	scanf("%d %d", &hour1, &minute1);
	printf("结束时间");
	scanf("%d %d", &hour2, &minute2);
	if (hour1 >= hour2 && minute1 > minute2)
	{
		ih = hour1 - hour2;
		im = minute1 = minute2;
	}
	else
	{
		ih = hour2 - hour1;
		im = minute2 - minute1;
	}
	if(im<0)
	{
		ih--;
		im = 60 + im;
	}
	printf("时差%d:%d", ih, im);
}
