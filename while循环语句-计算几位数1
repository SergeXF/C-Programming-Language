int test1(int i)
{
	int x;
	int n = 0;
	scanf("%d", &x);
	while (x > 0)
	{
		n++;
		x /= 10;
	}
	//printf("%d", n);
	return n;
}
int test2(int i)
{
	int x;
	long long int n = 0;
	scanf("%d", &x);
	while (x > 0)
	{
		n++;
		x /= 10;
	}
	//printf("%d", n);
	return n;
}
int main()
{
	int a=0;
	printf("请输入一个整数");
	printf("位数是%d",test1(a));
	printf("\n请再输入一个整数");
	printf("位数是%d", test2(a));
}
