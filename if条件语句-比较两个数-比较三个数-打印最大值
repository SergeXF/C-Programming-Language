int my_max2(int x)
{
	int a, b;
	scanf("%d %d", &a, &b);

	int max;
	if (a > b) max = a;
	else max = b;
	printf("The max is %d\n", max);
}
int my_max3(int x)
{
	int a, b, c;
	scanf("%d %d %d", &a, &b, &c);

	int max = 0;

	if (a > b) {
		if (a > c) {
			max = a;
		}
		else {
			max = c;
		}
	}
	else {
		if (b > c) {
			max = b;
		}
		else {
			max = c;
		}
	}

	printf("The max is %d\n", max);
}
int main()
{
	int a2 = 0;
	my_max2(a2);

	int a3 = 0;
	my_max3(a3);
	return 0;
}
